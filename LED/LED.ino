//********************************** Setup ****************************************
void setup()
{
  pinMode(2,OUTPUT);
}

//*********************************** Loop ****************************************
void loop ()
{
  digitalWrite(2, HIGH);
  delay(500);    
  digitalWrite(2,LOW);
  delay(500);
}

